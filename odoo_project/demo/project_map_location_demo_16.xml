        <!-- 16号线 -->
        <record id="project_project_wuhan_metro_line_16_station_1" model="project.map.location">
            <field name="sequence">1</field>
            <field name="active">True</field>
            <field name="name">国博中心南站</field>
            <field name="project_id" ref="odoo_project.project_project_wuhan_metro_line_16"/>
            <field name="description">位于博览中心南侧规划住宅用地内。博览中心站站址西侧有铁路专线箱涵（9m深U形槽结构）和拦江大道下穿隧道，南侧有正在施工的四新南路高架桥。本段现状场地多为荒地，车站主体周边5m范围内无地下管线，车站主体施工期间不需要进行交通疏解。博览中心站为双层三跨箱型结构，车站起点里程为右K8+081.360，终点里程为右K8+454.327。本站采用明挖法施工，基坑围护结构采用800mm厚连续墙，支撑采用一道钢筋砼支撑（局部两道）+3道钢管支撑+1道换撑，车站两端接盾构区间。</field>
            <field name="state_id" ref="base.state_cn_HB"/>
            <field name="city">武汉市</field>
            <field name="street">武汉市</field>
            <field name="street">武汉地铁16号线国博中心南站</field>
            <field name="country_id" ref="base.cn"/>
        </record>

        <record id="project_project_wuhan_metro_line_16_station_2" model="project.map.location">
            <field name="sequence">2</field>
            <field name="active">True</field>
            <field name="name">老关村站</field>
            <field name="project_id" ref="odoo_project.project_project_wuhan_metro_line_16"/>
            <field name="description">老关村站位于汉阳区三环线与武汉经济技术开发区沌口路交叉口西南侧，沿沌口路布置。该站东面为老关村委会；西面为四新混凝土拌合站；西南为华尔文气体有限公司和江堤市政办公楼。车站总长度357.0m，标准段宽22.1m。</field>
            <field name="state_id" ref="base.state_cn_HB"/>
            <field name="city">武汉市</field>
            <field name="street">武汉地铁16号线老关村站</field>
            <field name="country_id" ref="base.cn"/>
        </record>

