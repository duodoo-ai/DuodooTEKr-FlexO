<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- 系统里都是按 name 字符串搜索的，所以千万不要修改name -->
        <record id='decimal_signal' model="decimal.precision">
            <field name="name">Signal</field>
            <field name="digits" eval="2"/>
        </record>
        <record id='decimal_pressure' model="decimal.precision">
            <field name="name">Pressure</field>
            <field name="digits" eval="2"/>
        </record>
        <record id='decimal_traffic' model="decimal.precision">
            <field name="name">Traffic</field>
            <field name="digits" eval="2"/>
        </record>
        <record id='decimal_liquid' model="decimal.precision">
            <field name="name">Liquid</field>
            <field name="digits" eval="2"/>
        </record>
        <record id='decimal_temperature' model="decimal.precision">
            <field name="name">Temperature</field>
            <field name="digits" eval="2"/>
        </record>
        <record id='decimal_atmospheric' model="decimal.precision">
            <field name="name">Atmospheric</field>
            <field name="digits" eval="2"/>
        </record>
        <record id='decimal_humidity' model="decimal.precision">
            <field name="name">Humidity</field>
            <field name="digits" eval="2"/>
        </record>
        <record id='decimal_gps' model="decimal.precision">
            <field name="name">Gps</field>
            <field name="digits" eval="13"/>
        </record>
        <record id='decimal_gsm' model="decimal.precision">
            <field name="name">Gsm</field>
            <field name="digits" eval="13"/>
        </record>

        <record id='dtu_data_aggregate' model="dtu.data.aggregate">
            <field name="name">流量统计</field>
            <field name="monthly_traffic">0</field>
            <field name="quarterly_traffic">0</field>
            <field name="yearly_traffic">0</field>
            <field name="total_traffic">0</field>
        </record>
    </data>
</odoo>
